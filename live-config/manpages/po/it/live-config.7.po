#, no-wrap
msgid "B<hostname>"
msgstr "B<hostname>"

#. type: Plain text
#: en/live-config.7:184
msgid "configures /etc/hostname and /etc/hosts."
msgstr "configura i file /etc/hostname e /etc/hosts."

#. type: IP
#: en/live-config.7:184
#, no-wrap
msgid "B<user-setup>"
msgstr "B<user-setup>"

#. type: Plain text
